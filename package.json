{
  "name": "roundtable",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "description": "Roundtable Dashboard - Watch multiple models debate and brainstorm together",
  "author": "Roundtable <hello@roundtable.now>",
  "homepage": "https://roundtable.now/",
  "repository": {
    "type": "git",
    "url": "https://github.com/roundtable/dashboard"
  },
  "keywords": [
    "Roundtable",
    "AI collaboration",
    "brainstorming",
    "multiple models",
    "misaligned",
    "cloudflare",
    "cloudflare workers",
    "edge",
    "nextjs",
    "dashboard"
  ],
  "exports": {
    ".": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./next-dev": {
      "types": "./dist/next-dev/index.d.ts",
      "import": "./dist/next-dev/index.cjs",
      "require": "./dist/next-dev/index.cjs"
    }
  },
  "files": [
    "bin",
    "dist",
    "no-nodejs-compat-flag-static-error-page/dist",
    "templates"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "dev": "pnpm clean && pnpm local:clear-uploads && next dev --turbo",
    "dev:keep-uploads": "next dev --turbo",
    "email:dev": "email dev --dir src/emails --port 3001",
    "email:build": "email build --dir src/emails --out dist/emails",
    "email:export": "email export --dir src/emails --out dist/emails",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=7620' next build --webpack",
    "start": "next start",
    "clean": "rimraf .next .open-next out",
    "clean:all": "rimraf .next .swc out node_modules pnpm-lock.yaml .pnpm-store",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:modified": "git diff --name-only --diff-filter=ACM | grep -E '\\.(ts|tsx|js|jsx)$' | xargs -I {} sh -c 'echo \"Checking {}...\" && eslint {} --max-warnings 10'",
    "check-types": "cross-env NODE_OPTIONS='--max-old-space-size=7620' tsc --noEmit --pretty",
    "generate:icons": "npx @svgr/cli --memo --ref --typescript --jsx-runtime automatic --prettier-config .prettierrc --out-dir src/icons/component --icon --replace-attr-values -- src/icons/svg --ignore-existing",
    "generate:pwa-icons": "tsx scripts/generate-pwa-icons.ts",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "prebuild": "tsx scripts/prebuild.ts",
    "build:worker": "cross-env NODE_OPTIONS='--max-old-space-size=7620' pnpm prebuild && cross-env NODE_OPTIONS='--max-old-space-size=7620' opennextjs-cloudflare build",
    "preview:worker": "opennextjs-cloudflare preview",
    "preview": "pnpm build:worker && pnpm preview:worker",
    "deploy:preview": "pnpm build:worker && opennextjs-cloudflare deploy -- --env=preview --keep-vars",
    "deploy:production": "pnpm build:worker && opennextjs-cloudflare deploy -- --env=production --keep-vars",
    "deploy:prod": "pnpm deploy:production",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:migrate:preview": "wrangler d1 migrations apply DB --remote --env=preview",
    "db:migrate:prod": "wrangler d1 migrations apply DB --remote --env=production",
    "db:studio:local": "cross-env NEXT_PUBLIC_WEBAPP_ENV=local drizzle-kit studio",
    "db:studio:preview": "cross-env NEXT_PUBLIC_WEBAPP_ENV=preview drizzle-kit studio",
    "db:studio:prod": "cross-env NEXT_PUBLIC_WEBAPP_ENV=prod drizzle-kit studio",
    "db:seed:local": "wrangler d1 execute DB --local --file=./src/db/seed-local.sql",
    "db:seed:preview": "wrangler d1 execute DB --remote --env=preview --file=./src/db/seed-preview.sql",
    "db:seed:prod": "wrangler d1 execute DB --remote --env=production --file=./src/db/seed-prod.sql",
    "local:wipe-state": "rm -rf .wrangler/state .wrangler/tmp && echo '✅ Cleared all Wrangler state (D1, R2, KV, tmp)'",
    "local:clear-uploads": "rm -rf .wrangler/state/v3/r2 .local-uploads/* && (wrangler d1 execute DB --local --command=\"DELETE FROM project_attachment; DELETE FROM thread_upload; DELETE FROM message_upload; DELETE FROM upload;\" 2>/dev/null || true) && echo '✅ Cleared local uploads (R2 + .local-uploads + DB records)'",
    "db:full-reset:local": "pnpm local:wipe-state && pnpm db:migrate:local && pnpm db:seed:local && echo '✅ Database reset complete with seed data'",
    "local:nuclear-reset": "pnpm clean && pnpm local:wipe-state && pnpm db:migrate:local && pnpm db:seed:local && echo '✅ Nuclear reset complete: Next.js cache + all Wrangler state cleared'",
    "prepare": "husky",
    "knip": "knip",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "i18n:check-unused": "tsx scripts/find-unused-keys.ts",
    "i18n:find-hardcoded": "tsx scripts/find-hardcoded-strings.ts",
    "i18n:check-missing": "tsx scripts/check-missing-translations.ts",
    "i18n:audit": "tsx scripts/i18n-audit.ts",
    "i18n:fix": "tsx scripts/fix-translations.ts",
    "i18n:full-check": "pnpm i18n:validate && pnpm i18n:check-unused && pnpm i18n:find-hardcoded && pnpm i18n:check-missing",
    "test": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run",
    "test:watch": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest",
    "test:ui": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest --ui",
    "test:coverage": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run --coverage",
    "test:ci": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.7",
    "@cloudflare/puppeteer": "^1.0.4",
    "@hono/zod-openapi": "^1.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "1.14.7",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@react-email/components": "0.5.0",
    "@react-email/render": "1.2.0",
    "@scalar/hono-api-reference": "^0.9.30",
    "@scalar/openapi-to-markdown": "^0.3.12",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-virtual": "^3.13.16",
    "ai": "^6.0.7",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.4.10",
    "better-sqlite3": "^12.5.0",
    "chroma-js": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "fuse.js": "^7.1.0",
    "geist": "^1.5.1",
    "hono": "^4.11.3",
    "immer": "^11.1.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.27",
    "next": "16.0.10",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.6",
    "posthog-js": "^1.313.0",
    "posthog-node": "^5.18.1",
    "randomcolor": "^0.6.2",
