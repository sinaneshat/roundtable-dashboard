{
  "name": "roundtable",
  "version": "1.0.1",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "description": "Roundtable Dashboard - Watch multiple models debate and brainstorm together",
  "author": "Roundtable <hello@roundtable.now>",
  "homepage": "https://roundtable.now/",
  "repository": {
    "type": "git",
    "url": "https://github.com/roundtable/dashboard"
  },
  "keywords": [
    "Roundtable",
    "AI collaboration",
    "brainstorming",
    "multiple models",
    "misaligned",
    "cloudflare",
    "cloudflare workers",
    "edge",
    "nextjs",
    "dashboard"
  ],
  "exports": {
    ".": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./next-dev": {
      "types": "./dist/next-dev/index.d.ts",
      "import": "./dist/next-dev/index.cjs",
      "require": "./dist/next-dev/index.cjs"
    }
  },
  "files": [
    "bin",
    "dist",
    "no-nodejs-compat-flag-static-error-page/dist",
    "templates"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "dev": "pnpm clean && pnpm local:clear-uploads && next dev --turbo",
    "dev:keep-uploads": "next dev --turbo",
    "dev:email": "email dev --dir src/emails --port 3001",
    "email:preview": "email dev --dir src/emails --port 3001",
    "email:build": "email build --dir src/emails --out dist/emails",
    "email:export": "email export --dir src/emails --out dist/emails",
    "build": "node -v && cross-env NODE_OPTIONS='--max-old-space-size=8192' next build --webpack",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next .open-next out",
    "clean:all": "rimraf .next .swc out node_modules pnpm-lock.yaml .pnpm-store",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:file": "eslint --fix --max-warnings 10 --format unix",
    "lint:errors-only": "eslint --format compact | grep -E 'error' | wc -l | xargs",
    "lint:modified": "git diff --name-only --diff-filter=ACM | grep -E '\\.(ts|tsx|js|jsx)$' | xargs -I {} sh -c 'echo \"Checking {}...\" && eslint {} --max-warnings 10'",
    "check-types": "tsc --noEmit --pretty",
    "check-types:app": "tsc --noEmit --pretty",
    "generate:icons": "npx @svgr/cli --memo --ref --typescript --jsx-runtime automatic --prettier-config .prettierrc --out-dir src/icons/component --icon --replace-attr-values -- src/icons/svg --ignore-existing",
    "generate:pwa-icons": "tsx scripts/generate-pwa-icons.ts",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "cf-setup": "echo 'Setting up Cloudflare types...' && pnpm cf-typegen && echo 'Types generated successfully!'",
    "prebuild": "tsx scripts/prebuild.ts",
    "build:worker": "cross-env NODE_OPTIONS='--max-old-space-size=8192' pnpm prebuild && cross-env NODE_OPTIONS='--max-old-space-size=8192' opennextjs-cloudflare build",
    "cache:clear:local": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --local --file=./scripts/clear-tag-cache.sql",
    "cache:clear:preview": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --remote --env=preview --file=./scripts/clear-tag-cache.sql",
    "cache:clear:prod": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --remote --env=production --file=./scripts/clear-tag-cache.sql",
    "cache:clear-r2:preview": "npx tsx scripts/clear-r2-cache.ts preview",
    "cache:clear-r2:prod": "npx tsx scripts/clear-r2-cache.ts prod",
    "cache:clear-all:preview": "pnpm cache:clear:preview && pnpm cache:clear-r2:preview",
    "cache:clear-all:prod": "pnpm cache:clear:prod && pnpm cache:clear-r2:prod",
    "preview:worker": "opennextjs-cloudflare preview",
    "preview": "pnpm build:worker && pnpm preview:worker",
    "deploy:worker": "opennextjs-cloudflare deploy -- --keep-vars",
    "deploy": "pnpm build:worker && pnpm deploy:worker",
    "upload": "pnpm build:worker && opennextjs-cloudflare upload -- --keep-vars",
    "deploy:preview": "pnpm tag-cache:init:preview && pnpm build:worker && opennextjs-cloudflare deploy -- --env=preview --keep-vars",
    "deploy:production": "pnpm tag-cache:init:prod && pnpm build:worker && opennextjs-cloudflare deploy -- --env=production --keep-vars",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:drop-all:local": "rm -rf .wrangler/state/v3/d1/*",
    "db:push:local": "npx wrangler d1 execute DB --local --file=$(ls ./src/db/migrations/*.sql | head -n 1)",
    "db:push:preview": "npx wrangler d1 execute DB --remote --preview --file=$(ls ./src/db/migrations/*.sql | head -n 1)",
    "db:push:prod": "npx wrangler d1 execute DB --remote --file=$(ls ./src/db/migrations/*.sql | head -n 1)",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:migrate:preview": "wrangler d1 migrations apply DB --remote --env=preview",
    "db:migrate:prod": "wrangler d1 migrations apply DB --remote",
    "db:studio:local": "cross-env NEXT_PUBLIC_WEBAPP_ENV=local drizzle-kit studio",
    "db:studio:preview": "cross-env NEXT_PUBLIC_WEBAPP_ENV=preview drizzle-kit studio",
    "db:studio:prod": "cross-env NEXT_PUBLIC_WEBAPP_ENV=prod drizzle-kit studio",
    "db:seed:local": "npx wrangler d1 execute DB --local --file=./src/db/seed-local.sql",
    "db:seed:preview": "npx wrangler d1 execute DB --remote --env=preview --file=./src/db/seed-preview.sql",
    "db:seed:prod": "npx wrangler d1 execute DB --remote --file=./src/db/seed-prod.sql",
    "db:drop-all:preview": "npx wrangler d1 execute DB --remote --env=preview --file=./src/db/drop_all.sql",
    "db:drop-all:prod": "npx wrangler d1 execute DB --remote --env=production --file=./src/db/drop_all.sql",
    "local:wipe-state": "rm -rf .wrangler/state .wrangler/tmp && echo '✅ Cleared all Wrangler state (D1, R2, KV, tmp)'",
    "local:clear-uploads": "rm -rf .wrangler/state/v3/r2 .local-uploads/* && (wrangler d1 execute DB --local --command=\"DELETE FROM project_attachment; DELETE FROM thread_upload; DELETE FROM message_upload; DELETE FROM upload;\" 2>/dev/null || true) && echo '✅ Cleared local uploads (R2 + .local-uploads + DB records)'",
    "db:full-reset:local": "pnpm local:wipe-state && pnpm db:migrate:local && pnpm db:seed:local && pnpm tag-cache:init:local && echo '✅ Full reset complete: DB + all caches cleared'",
    "db:full-reset:preview": "pnpm db:drop-all:preview && pnpm db:migrate:preview && pnpm db:seed:preview && pnpm tag-cache:init:preview && pnpm cache:clear:preview && echo '✅ Preview full reset complete: DB + all caches cleared'",
    "db:full-reset:prod": "pnpm db:drop-all:prod && pnpm db:migrate:prod && pnpm db:seed:prod && pnpm tag-cache:init:prod && pnpm cache:clear:prod && echo '✅ Prod full reset complete: DB + all caches cleared'",
    "local:nuclear-reset": "pnpm clean && pnpm local:wipe-state && pnpm db:migrate:local && pnpm db:seed:local && pnpm tag-cache:init:local && echo '✅ Nuclear reset complete: Next.js + all Wrangler state + caches cleared'",
    "tag-cache:init:local": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --local --file=./scripts/init-tag-cache.sql",
    "tag-cache:init:preview": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --remote --env=preview --file=./scripts/init-tag-cache.sql",
    "tag-cache:init:prod": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --remote --env=production --file=./scripts/init-tag-cache.sql",
    "tag-cache:view:local": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --local --command \"SELECT * FROM revalidations ORDER BY revalidatedAt DESC LIMIT 10\"",
    "tag-cache:view:preview": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --remote --env=preview --command \"SELECT * FROM revalidations ORDER BY revalidatedAt DESC LIMIT 10\"",
    "tag-cache:view:prod": "npx wrangler d1 execute NEXT_TAG_CACHE_D1 --remote --env=production --command \"SELECT * FROM revalidations ORDER BY revalidatedAt DESC LIMIT 10\"",
    "blog:schema": "tsc -p tsconfig.json src/tools/blog-generator/schema.ts",
    "blog:generate": "chmod +x ./src/tools/blog-generator/generate-blog.sh && ./src/tools/blog-generator/generate-blog.sh",
    "blog:generate:ts": "tsx src/tools/blog-generator/run-blog-generator.ts",
    "blog:add-to-db:local": "LAST_BLOG=$(ls -t ./src/tools/blog-generator/output/*.sql | head -1) && npx wrangler d1 execute DB --local --file=$LAST_BLOG",
    "blog:add-to-db:preview": "LAST_BLOG=$(ls -t ./src/tools/blog-generator/output/*.sql | head -1) && npx wrangler d1 execute DB --remote --preview --file=$LAST_BLOG",
    "blog:add-to-db:prod": "LAST_BLOG=$(ls -t ./src/tools/blog-generator/output/*.sql | head -1) && npx wrangler d1 execute DB --remote --file=$LAST_BLOG",
    "prepare": "husky",
    "knip": "knip",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "i18n:check-unused": "tsx scripts/find-unused-keys.ts",
    "i18n:find-hardcoded": "tsx scripts/find-hardcoded-strings.ts",
    "i18n:check-missing": "tsx scripts/check-missing-translations.ts",
    "i18n:audit": "tsx scripts/i18n-audit.ts",
    "i18n:fix": "tsx scripts/fix-translations.ts",
    "i18n:full-check": "npm run i18n:validate && npm run i18n:check-unused && npm run i18n:find-hardcoded && npm run i18n:check-missing",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.5",
    "@cloudflare/puppeteer": "^1.0.4",
    "@hono/zod-openapi": "^1.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "1.14.7",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@react-email/components": "0.5.0",
    "@react-email/render": "1.2.0",
    "@scalar/hono-api-reference": "^0.9.30",
    "@scalar/openapi-to-markdown": "^0.3.12",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-virtual": "^3.13.14",
    "ai": "^6.0.5",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.4.9",
    "better-sqlite3": "^12.5.0",
    "chroma-js": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "0.8.3",
    "fuse.js": "^7.1.0",
    "geist": "^1.5.1",
    "hono": "^4.11.3",
    "immer": "^11.1.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "16.1.1",
    "next-intl": "^4.6.1",
    "next-share": "^0.27.0",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.6",
    "posthog-js": "^1.313.0",
    "posthog-node": "^5.18.1",
    "randomcolor": "^0.6.2",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-email": "4.3.1",
    "react-hook-form": "^7.69.0",
    "react-markdown": "^10.1.0",
    "recharts": "3.6.0",
    "shiki": "^3.20.0",
    "simplex-noise": "^4.0.3",
    "stoker": "^2.0.1",
    "streamdown": "^1.6.10",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "ulid": "^3.0.2",
    "unpdf": "^1.4.0",
    "use-stick-to-bottom": "^1.1.1",
    "vaul": "^1.1.2",
    "zod": "^4.3.2",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@cloudflare/workers-types": "^4.20251231.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/cz-commitlint": "^20.2.0",
    "@commitlint/types": "^20.2.0",
    "@eslint-react/eslint-plugin": "^2.5.0",
    "@next/eslint-plugin-next": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@react-email/preview-server": "4.3.1",
    "@sparticuz/chromium": "^143.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/chroma-js": "^3.1.2",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^25.0.3",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "inquirer": "^13.1.0",
    "jsdom": "^27.4.0",
    "knip": "^5.78.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "puppeteer-core": "^24.34.0",
    "rimraf": "^6.1.2",
    "shadcn": "^3.6.2",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "wrangler": "^4.54.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "workerd"
    ],
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3",
      "core-js",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd",
      "yarn"
    ],
    "overrides": {},
    "patchedDependencies": {
      "@opennextjs/cloudflare@1.14.7": "patches/@opennextjs__cloudflare@1.14.7.patch"
    }
  }
}
