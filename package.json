  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "puppeteer",
      "workerd"
    ],
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3",
      "core-js",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd",
      "yarn"
    ]
  }
