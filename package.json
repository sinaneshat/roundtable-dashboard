{
  "name": "roundtable",
  "version": "1.5.0",
  "private": true,
  "packageManager": "bun@1.3.6",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "description": "Roundtable Dashboard - Watch multiple models debate and brainstorm together",
  "author": "Roundtable <soheil@deadpixel.ai>",
  "homepage": "https://roundtable.now/",
  "repository": {
    "type": "git",
    "url": "https://github.com/roundtable/dashboard"
  },
  "keywords": [
    "Roundtable",
    "AI collaboration",
    "brainstorming",
    "multiple models",
    "misaligned",
    "cloudflare",
    "cloudflare workers",
    "edge",
    "tanstack-start",
    "dashboard"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "turbo:dev": "turbo run dev",
    "turbo:build": "turbo run build",
    "turbo:lint": "turbo run lint",
    "turbo:check-types": "turbo run check-types",
    "turbo:test": "turbo run test",
    "api:dev": "turbo run dev --filter=@roundtable/api",
    "api:build": "turbo run build --filter=@roundtable/api",
    "api:deploy:preview": "turbo run deploy:preview --filter=@roundtable/api",
    "api:deploy:production": "turbo run deploy:production --filter=@roundtable/api",
    "web:dev": "turbo run dev --filter=@roundtable/web",
    "web:build": "turbo run build --filter=@roundtable/web",
    "web:deploy:preview": "turbo run deploy:preview --filter=@roundtable/web",
    "web:deploy:production": "turbo run deploy:production --filter=@roundtable/web",
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=@roundtable/api",
    "dev:web": "turbo run dev --filter=@roundtable/web",
    "email:dev": "email dev --dir apps/api/src/emails --port 3001",
    "email:build": "email build --dir apps/api/src/emails --out dist/emails",
    "email:export": "email export --dir apps/api/src/emails --out dist/emails",
    "build": "turbo run build",
    "clean": "rimraf apps/web/.output apps/api/dist",
    "clean:all": "rimraf node_modules bun.lockb .bun apps/*/node_modules packages/*/node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:modified": "git diff --name-only --diff-filter=ACM | grep -E '\\.(ts|tsx|js|jsx)$' | xargs -I {} sh -c 'echo \"Checking {}...\" && eslint {} --max-warnings 10'",
    "check-types": "turbo run check-types",
    "cf-typegen": "cd apps/api && bun run cf-typegen && cd ../web && bun run cf-typegen",
    "cf-typegen:check": "cd apps/api && bun run cf-typegen && git diff --exit-code cloudflare-env.d.ts || (echo '⚠️  cloudflare-env.d.ts is out of date. Run bun run cf-typegen and commit.' && exit 1)",
    "generate:icons": "npx @svgr/cli --memo --ref --typescript --jsx-runtime automatic --prettier-config .prettierrc --out-dir apps/web/src/icons/component --icon --replace-attr-values -- apps/web/src/icons/svg --ignore-existing",
    "preview": "turbo run preview",
    "deploy:preview": "turbo run deploy:preview",
    "deploy:production": "turbo run deploy:production",
    "db:generate": "cd apps/api && bunx drizzle-kit generate",
    "db:migrate:local": "cd apps/api && bunx wrangler d1 migrations apply DB --local",
    "db:migrate:preview": "cd apps/api && bunx wrangler d1 migrations apply DB --remote --env=preview",
    "db:migrate:prod": "cd apps/api && bunx wrangler d1 migrations apply DB --remote --env=production",
    "db:studio:local": "cd apps/api && cross-env WEBAPP_ENV=local bunx drizzle-kit studio",
    "db:studio:preview": "cd apps/api && cross-env WEBAPP_ENV=preview bunx drizzle-kit studio",
    "db:studio:prod": "cd apps/api && cross-env WEBAPP_ENV=prod bunx drizzle-kit studio",
    "db:seed:local": "cd apps/api && bunx wrangler d1 execute DB --local --file=./src/db/seed-local.sql",
    "db:seed:preview": "cd apps/api && bunx wrangler d1 execute DB --remote --env=preview --file=./src/db/seed-preview.sql",
    "db:seed:prod": "cd apps/api && bunx wrangler d1 execute DB --remote --env=production --file=./src/db/seed-prod.sql",
    "local:wipe-state": "rm -rf .wrangler/state apps/api/.wrangler/state && echo '✅ Cleared all Wrangler state (D1, R2, KV) from root + apps/api'",
    "local:clear-uploads": "rm -rf .wrangler/state/v3/r2 apps/api/.wrangler/state/v3/r2 .local-uploads/* && (cd apps/api && bunx wrangler d1 execute DB --local --command=\"DELETE FROM project_attachment; DELETE FROM thread_upload; DELETE FROM message_upload; DELETE FROM upload;\" 2>/dev/null || true) && echo '✅ Cleared local uploads (R2 + .local-uploads + DB records)'",
    "db:full-reset:local": "bun run local:wipe-state && bun run db:migrate:local && bun run db:seed:local && echo '✅ Database reset complete with seed data'",
    "local:nuclear-reset": "bun run clean && bun run local:wipe-state && bun run db:migrate:local && bun run db:seed:local && echo '✅ Nuclear reset complete: build cache + all Wrangler state cleared'",
    "prepare": "husky",
    "knip": "knip",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "i18n:check-unused": "tsx scripts/find-unused-keys.ts",
    "i18n:find-hardcoded": "tsx scripts/find-hardcoded-strings.ts",
    "i18n:check-missing": "tsx scripts/check-missing-translations.ts",
    "i18n:find-missing": "tsx scripts/find-missing-i18n-keys.ts",
    "i18n:add-missing": "tsx scripts/add-missing-i18n-keys.ts",
    "i18n:audit": "tsx scripts/i18n-audit.ts",
    "i18n:fix": "tsx scripts/fix-translations.ts",
    "i18n:full-check": "bun run i18n:validate && bun run i18n:check-unused && bun run i18n:find-hardcoded && bun run i18n:find-missing",
    "test": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run",
    "test:watch": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest",
    "test:ui": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest --ui",
    "test:coverage": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run --coverage",
    "test:ci": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "user:upgrade": "tsx scripts/user-upgrade.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:tag": "changeset tag",
    "release": "turbo run build lint check-types && changeset version"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.7",
    "@cloudflare/puppeteer": "^1.0.4",
