{
  "extends": "@roundtable/typescript-config/react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noEmit": true
  },
  "include": ["src/**/*", "src/types/**/*.d.ts", "*.d.ts", "vite.config.ts"],
  "exclude": [
    "node_modules",
    ".output",
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx"
  ]
}
