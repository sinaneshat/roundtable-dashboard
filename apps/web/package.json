{
  "name": "@roundtable/web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "prebuild": "bun run cf-typegen || true",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "build:local": "VITE_WEBAPP_ENV=local bun run build",
    "build:preview": "VITE_WEBAPP_ENV=preview bun run build",
    "build:production": "VITE_WEBAPP_ENV=prod bun run build",
    "preview": "bun run build && vite preview",
    "deploy": "bun run build:local && wrangler deploy",
    "deploy:preview": "bun run build:preview && wrangler deploy --env=preview",
    "deploy:production": "bun run build:production && wrangler deploy --env=production",
    "cf-typegen": "wrangler types",
    "check-types": "./scripts/check-types.sh",
    "check-types:strict": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_OPTIONS='--max-old-space-size=2048' vitest run",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=2048' vitest",
    "test:sequential": "NODE_OPTIONS='--max-old-space-size=1024' vitest run --no-file-parallelism --maxWorkers=1",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=2048' CI=true vitest run --maxWorkers=2 --no-file-parallelism",
    "remotion:studio": "remotion studio src/remotion/index.ts --config src/remotion/remotion.config.ts",
    "remotion:render": "remotion render src/remotion/index.ts --config src/remotion/remotion.config.ts",
    "remotion:render:ph": "remotion render src/remotion/index.ts ProductHuntDemo --config src/remotion/remotion.config.ts out/ph-demo.mp4"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
