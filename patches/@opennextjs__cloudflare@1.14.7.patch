+++ b/dist/cli/build/patches/plugins/fast-set-immediate.js
@@ -0,0 +1,22 @@
+import { join } from "node:path";
+import { getCrossPlatformPathRegex } from "@opennextjs/aws/utils/regex.js";
+
+/**
+ * Shims the fast-set-immediate module to prevent "Cannot assign to read only property 'setImmediate'" error
+ * in Cloudflare Workers with Next.js 16.
+ */
+export function shimFastSetImmediate(options) {
+    const emptyShimPath = join(options.outputDir, "cloudflare-templates/shims/empty.js");
+    return {
+        name: "shimFastSetImmediate",
+        setup(build) {
+            // Shim the fast-set-immediate.external module that tries to modify globalThis.setImmediate
+            build.onResolve(
+                { filter: getCrossPlatformPathRegex(String.raw`fast-set-immediate\.external`, { escape: false }) },
+                () => ({
+                    path: emptyShimPath,
+                })
+            );
+        },
+    };
+}
